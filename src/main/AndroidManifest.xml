<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.insanus.course.try_module_event">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".event.ButtonEventActivity" />
        <activity android:name=".event.PlaneGameActivity" />
        <activity android:name=".event.CallBackActivity" />
        <activity android:name=".event.CallBackPlaneActivity" />
        <activity android:name=".event.TryConfigurationActivity" />
        <activity
            android:name=".event.ListenerConfigurationActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".event.TryHandlerActivity" />
        <activity android:name=".event.MessageOperationActivity" />
        <activity android:name=".event.AsyncTaskActivity" />
    </application>

</manifest>